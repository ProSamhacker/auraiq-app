{
  "crons": [
    {
      "path": "/api/cron/cleanup-blobs",
      "schedule": "0 2 * * *"
    }
  ],
  
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors *" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}


  